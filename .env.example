# Example environment file for build-time API configuration
# Copy to .env and set your CasaTunes host (protocol + host only, no port, no trailing slash, no /api/v1)
REACT_APP_API_BASE=http://casaserver.local

# Optional: specify a custom CasaTunes API port (defaults to 8735 if unset)
REACT_APP_API_PORT=8735

# Application port: default 8080
DOCKER_APP_PORT=8080
